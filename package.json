{
  "name": "app-playlist-creator",
  "version": "1.0.0",
  "scripts": {
    "start": "blitz start",
    "studio": "blitz db studio",
    "build": "blitz build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress:open": "cypress open",
    "test:e2e": "start-server-and-test start:cli http://localhost:3000/list cy:open"
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/cli": "~2.12",
    "@prisma/client": "~2.12",
    "@types/react-query": "1.1.2",
    "@types/spotify-api": "0.0.5",
    "@types/styletron-engine-atomic": "1.1.0",
    "@types/styletron-react": "5.0.2",
    "@types/styletron-standard": "2.0.1",
    "baseui": "9.106.3",
    "blitz": "0.27.1",
    "cypress": "6.1.0",
    "final-form": "4.20.1",
    "framer": "1.2.11",
    "framer-motion": "3.0.0",
    "node-fetch": "2.6.1",
    "react": "0.0.0-experimental-4ead6b530",
    "react-audio-player": "0.14.0",
    "react-dom": "0.0.0-experimental-4ead6b530",
    "react-error-boundary": "2.3.2",
    "react-final-form": "6.5.2",
    "react-query": "3.3.2",
    "react-spotify-api": "3.0.0",
    "react-spotify-web-playback": "0.8.1",
    "secure-password": "4.0.0",
    "styletron-engine-atomic": "1.4.6",
    "styletron-react": "5.2.7",
    "typescript": "4.1.3",
    "zod": "1.11.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "6.1.11",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/react-hooks": "3.7.0",
    "@types/jest": "26.0.19",
    "@types/react": "16.14.2",
    "@types/secure-password": "3.1.0",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "7.15.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-baseui": "9.106.3",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-fetch-mock": "3.0.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.3",
    "msw": "0.24.2",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react-test-renderer": "16.14.0",
    "ts-jest": "26.4.4"
  },
  "private": true
}
